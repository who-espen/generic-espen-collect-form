<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>South Sudan:  F2 - Individual Participant Survey Form - LF/LOA-LOA</h:title>
    <model>
      <instance>
        <sd-f2-lf-participants.v2 id="sd-f2-lfparticipants" version="3">
          <p_recorder_id/>
          <p_village_id/>
          <p_particiant_id_generated/>
          <p_particiant_id_generated2/>
          <p_enrollment/>
          <p_sex/>
          <p_age/>
          <p_lived_in_village/>
          <p_sleep_under_net/>
          <p_morbidity/>
          <p_experienced_worm/>
          <p_had_picture_condition/>
          <p_duration_in_eye/>
          <p_remarks/>
          <p_saved_at/>
          <meta>
            <instanceID/>
          </meta>
        </sd-f2-lf-participants.v2>
      </instance>
      <bind constraint="regex(.,'^[0-9]{2}$')" jr:constraintMsg="Invalid. Recorder ID code is 2 numbers long." jr:requiredMsg="you must enter the recorder ID" nodeset="/sd-f2-lf-participants.v2/p_recorder_id" required="true()" type="int"/>
      <bind constraint="regex(.,'^[0-9]{3}$')" jr:constraintMsg="Invalid. Recorder ID code is 3 numbers long." jr:requiredMsg="You must select enter the village codde" nodeset="/sd-f2-lf-participants.v2/p_village_id" required="true()" type="int"/>
      <bind constraint="regex(., '^[0-9]{3}-[0-9]{4}$')" jr:constraintMsg="The format must be 123-1234" nodeset="/sd-f2-lf-participants.v2/p_particiant_id_generated" required="true()" type="text"/>
      <bind constraint=".= /sd-f2-lf-participants.v2/p_particiant_id_generated " jr:constraintMsg="The ID is not equal to the generated ID" nodeset="/sd-f2-lf-participants.v2/p_particiant_id_generated2" required="true()" type="text"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_enrollment" required="true()" type="select1"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_sex" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;" required="true()" type="select1"/>
      <bind constraint=".&gt;=15 and .&lt;135" jr:constraintMsg="The age you must be between 15 and 135 years" nodeset="/sd-f2-lf-participants.v2/p_age" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;" required="true()" type="int"/>
      <bind constraint=".&lt;= /sd-f2-lf-participants.v2/p_age " jr:constraintMsg="The time lived in the village is greater the the participant age" nodeset="/sd-f2-lf-participants.v2/p_lived_in_village" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;" required="true()" type="int"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_sleep_under_net" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;and  /sd-f2-lf-participants.v2/p_lived_in_village  &gt;= 10" required="true()" type="select1"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_morbidity" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;and  /sd-f2-lf-participants.v2/p_lived_in_village  &gt;= 10" required="true()" type="select1"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_experienced_worm" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;and  /sd-f2-lf-participants.v2/p_lived_in_village  &gt;= 10" required="true()" type="select1"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_had_picture_condition" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot;and  /sd-f2-lf-participants.v2/p_lived_in_village  &gt;= 10" required="true()" type="select1"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_duration_in_eye" relevant=" /sd-f2-lf-participants.v2/p_enrollment =&quot;1_Consent&quot; and  /sd-f2-lf-participants.v2/p_lived_in_village  &gt;= 10 and  /sd-f2-lf-participants.v2/p_had_picture_condition =&quot;Yes&quot;" required="true()" type="int"/>
      <bind nodeset="/sd-f2-lf-participants.v2/p_remarks" required="false()" type="text"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/sd-f2-lf-participants.v2/p_saved_at" type="dateTime"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/sd-f2-lf-participants.v2/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/sd-f2-lf-participants.v2/p_recorder_id">
      <label>Recorder ID</label>
      <hint>2 digit code assigned to you</hint>
    </input>
    <input ref="/sd-f2-lf-participants.v2/p_village_id">
      <label>Enter the village code</label>
      <hint>3 digit code assigned to this village</hint>
    </input>
    <input ref="/sd-f2-lf-participants.v2/p_particiant_id_generated">
      <label>Enter unique ID from the diagnostic test (must be entered as 123-1234 digit only with dashes)adminstered, including FTS and Dry Blood Spot tests.</label>
    </input>
    <input ref="/sd-f2-lf-participants.v2/p_particiant_id_generated2">
      <label>Repeat unique ID (must be entered as 123-1234 digit only with dashes)</label>
    </input>
    <select1 ref="/sd-f2-lf-participants.v2/p_enrollment">
      <label>Select enrollment type</label>
      <item>
        <label>1 Consent</label>
        <value>1_Consent</value>
      </item>
      <item>
        <label>2 Absent</label>
        <value>2_Absent</value>
      </item>
      <item>
        <label>3 Refused</label>
        <value>3_Refused</value>
      </item>
    </select1>
    <select1 ref="/sd-f2-lf-participants.v2/p_sex">
      <label>Select the sex</label>
      <item>
        <label>1 Male</label>
        <value>1_Male</value>
      </item>
      <item>
        <label>2 Female</label>
        <value>2_Female</value>
      </item>
    </select1>
    <input ref="/sd-f2-lf-participants.v2/p_age">
      <label>Enter the age</label>
    </input>
    <input ref="/sd-f2-lf-participants.v2/p_lived_in_village">
      <label>How long have you lived in this village?</label>
      <hint>He will be exluded if he lived less than four years in the village.</hint>
    </input>
    <select1 ref="/sd-f2-lf-participants.v2/p_sleep_under_net">
      <label>Did you sleep under a bednet last night?</label>
      <item>
        <label>1 Yes</label>
        <value>Yes</value>
      </item>
      <item>
        <label>2 No</label>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/sd-f2-lf-participants.v2/p_morbidity">
      <label>Morbidity</label>
      <item>
        <label>1 Hydrocele</label>
        <value>1_Hydrocele</value>
      </item>
      <item>
        <label>2 Lympedema</label>
        <value>2_Lympedema</value>
      </item>
      <item>
        <label>3 Chyluria</label>
        <value>3_chyluria</value>
      </item>
      <item>
        <label>4 Null</label>
        <value>N/A</value>
      </item>
    </select1>
    <select1 ref="/sd-f2-lf-participants.v2/p_experienced_worm">
      <label>Have you ever experienced or noticed worms move along the white of your eye?</label>
      <item>
        <label>1 Yes</label>
        <value>Yes</value>
      </item>
      <item>
        <label>2 No</label>
        <value>No</value>
      </item>
    </select1>
    <select1 ref="/sd-f2-lf-participants.v2/p_had_picture_condition">
      <label>Have you ever had the condition in this picture?</label>
      <item>
        <label>1 Yes</label>
        <value>Yes</value>
      </item>
      <item>
        <label>2 No</label>
        <value>No</value>
      </item>
    </select1>
    <input ref="/sd-f2-lf-participants.v2/p_duration_in_eye">
      <label>How long ago did you last experience it (in days)?</label>
    </input>
    <input ref="/sd-f2-lf-participants.v2/p_remarks">
      <label>Remarks</label>
    </input>
  </h:body>
</h:html>
